package santosh;

public class ArithmaticOperators
{

	public static void main(String[] args)
	{
		// TODO Auto-generated method stub
		int num1 = 10;
		int num2 = 20;
		System.out.println(num1+num2);
		System.out.println(num1-num2);
		System.out.println(num1*num2);
		System.out.println(num1/num2);
		System.out.println(num1%num2);
		System.out.println(num1);
		System.out.println(num1++);
	}

}
